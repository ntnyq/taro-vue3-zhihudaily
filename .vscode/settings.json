{
  "eslint.validate": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "vue",
    "yaml",
    "html",
    "markdown",
    "json",
    "jsonc",
    "json5"
  ],
  "cSpell.allowCompoundWords": true,
  "cSpell.words": [
    "avenir",
    "bumpp",
    "changelogithub",
    "esbuild",
    "izzyleung",
    "ldquo",
    "nutui",
    "pinia",
    "pnpm",
    "pxtransform",
    "rdquo",
    "styl",
    "tarojs",
    "walleeeee",
    "weapp",
    "Zhihu"
  ],
  "[javascript]": {
    "editor.formatOnSave": false
  },
  "[typescript]": {
    "editor.formatOnSave": false
  },
  "[vue]": {
    "editor.formatOnSave": false
  }
}
