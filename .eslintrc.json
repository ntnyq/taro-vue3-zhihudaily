{
  "root": true,
  "extends": ["@ntnyq"],
  "rules": {
    "vue/prefer-true-attribute-shorthand": "off",
    "vue/component-name-in-template-casing": [
      "error",
      "kebab-case",
      {
        "registeredComponentsOnly": false,
        "ignores": []
      }
    ]
  },
  "ignorePatterns": ["src/index.html"]
}
