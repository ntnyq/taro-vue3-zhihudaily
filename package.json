{
  "name": "starter-taro-vue",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@7.11.0",
  "description": "starter-taro-vue",
  "templateInfo": {
    "name": "vue3-NutUI",
    "typescript": true,
    "css": "sass"
  },
  "eslintIgnore": [
    "src/index.html"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@babel/runtime": "^7.19.0",
    "@nutui/nutui-taro": "^3.2.2",
    "@tarojs/components": "3.5.5",
    "@tarojs/plugin-framework-vue3": "3.5.5",
    "@tarojs/plugin-html": "3.5.5",
    "@tarojs/runtime": "3.5.5",
    "@tarojs/taro": "3.5.5",
    "axios-miniprogram": "2.0.0-rc-2",
    "pinia": "^2.0.22",
    "taro-plugin-pinia": "^1.0.0",
    "vue": "^3.2.39"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@ntnyq/eslint-config": "^1.0.4",
    "@tarojs/cli": "3.5.5",
    "@tarojs/mini-runner": "3.5.5",
    "@tarojs/webpack-runner": "3.5.5",
    "@types/webpack-env": "^1.18.0",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/compiler-sfc": "^3.2.39",
    "babel-plugin-import": "^1.13.5",
    "babel-preset-taro": "3.5.5",
    "eslint": "^8.23.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "pnpm": "^7.11.0",
    "typescript": "^4.8.3",
    "vue-loader": "^17.0.0",
    "vue-tsc": "^0.40.13"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@ntnyq",
    "rules": {
      "vue/prefer-true-attribute-shorthand": "off"
    }
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "webpack"
      ]
    }
  }
}
